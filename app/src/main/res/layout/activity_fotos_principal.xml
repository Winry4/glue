<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_content"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#14394E"
    android:fitsSystemWindows="true"
    tools:context="srinternet.glue.fotosPrincipal">

    <android.support.v4.view.ViewPager
        android:id="@+id/container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentStart="true"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        android:layout_above="@+id/textView54"
        android:layout_below="@+id/imageView2" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/boton_cerrar_fotos"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true" />

    <TextView
        android:id="@+id/textView54"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="102dp" />

    <ImageView
        android:id="@+id/punto_foto_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="98dp"
        app:srcCompat="@drawable/punto_b_fotos"
        android:layout_below="@+id/container"
        android:layout_toLeftOf="@+id/imageView2"
        android:layout_toStartOf="@+id/imageView2" />

    <ImageView
        android:id="@+id/punto_foto_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="25dp"
        android:layout_marginStart="25dp"
        app:srcCompat="@drawable/punto_b_fotos"
        android:layout_below="@+id/container"
        android:layout_alignLeft="@+id/punto_foto_2"
        android:layout_alignStart="@+id/punto_foto_2" />

    <ImageView
        android:id="@+id/punto_foto_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/container"
        android:layout_marginRight="11dp"
        android:layout_toLeftOf="@+id/punto_foto_2"
        android:layout_toStartOf="@+id/punto_foto_2"
        app:srcCompat="@drawable/punto_b_fotos" />

    <ImageView
        android:id="@+id/punto_foto_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/container"
        android:layout_marginLeft="11dp"
        android:layout_marginStart="11dp"
        android:layout_toRightOf="@+id/punto_foto_3"
        app:srcCompat="@drawable/punto_b_fotos" />

</RelativeLayout>
