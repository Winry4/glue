        android:textColor="#14394E" />

    <ImageView
        android:id="@+id/imageView43"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/sombra_menu_b"
        android:layout_above="@+id/btn_mujeres"
        android:layout_marginBottom="33dp"
        android:layout_alignRight="@+id/btn_hombres"
        android:layout_alignLeft="@+id/btn_hombres" />

    <RelativeLayout
        android:id="@+id/relativeLayout2"
        android:layout_width="350dp"
        android:layout_height="110dp"
        android:layout_alignLeft="@+id/textView29"
        android:layout_below="@+id/textView29"
